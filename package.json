{
  "name": "archeryapp",
  "version": "1.0.0",
  "description": "Database and adapters",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run client:build",
    "seed:dev": "nodemon -L ./db/seed.js",
    "client:dev": "CHOKIDAR_USEPOLLING=true react-scripts start",
    "client:build": "react-scripts build",
    "server:dev": "nodemon -L index.js",
    "db:build": "node ./db/seed.js",
    "start:dev": "concurrently \"npm run client:build\" \"npm run client:dev\" \"npm run server:dev\"",
    "start": "node index.js",
    "test": "jest --runInBand",
    "test:watch": "jest --watchAll --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nmullen25/ArcheryApp.git"
  },
  "author": "Nicholas Mullen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nmullen25/ArcheryApp/issues"
  },
  "devDependencies": {
    "nodemon": "^2.0.15",
    "supertest": "^6.2.1"
  },
  "proxy": "http://localhost:4000",
  "dependencies": {
    "@stripe/react-stripe-js": "^1.7.2",
    "@stripe/stripe-js": "^1.29.0",
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.20.0",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "react-stripe-checkout": "^2.6.3",
    "stripe": "^9.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
